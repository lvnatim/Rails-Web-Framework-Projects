<h2> Overview for <%= @user.full_name %> </h2>
<h3>Email: <%= @user.email %> </h3>


<h4> Reviews </h4>
<% @user.reviews.each do |review| %>
    <h4><%= link_to review.movie.title, movie_path(review.movie) %> | <%= formatted_date(review.created_at) %></h4>
    <p><%= review.text %> - <%= review.rating_out_of_ten %>/10</p>
<% end %>
<br>
